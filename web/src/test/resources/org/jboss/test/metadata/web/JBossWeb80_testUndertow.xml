<?xml version="1.0" encoding="UTF-8"?>
<jboss-web version="8.0" xmlns="http://www.jboss.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.jboss.org/j2ee/schema/jboss-web_8_0.xsd">

    <executor-name>myexecutor</executor-name>
    <shared-session-manager>sessionManager</shared-session-manager>
    <servlet-container>default</servlet-container>
    <server-instance>default</server-instance>
    <default-encoding>UTF-8</default-encoding>
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <executor-name>otherexecutor</executor-name>
    </servlet>
    <jacc-context-id>myContextID</jacc-context-id>
</jboss-web>